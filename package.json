{
  "name": "ts-express-app",
  "version": "1.0.0",
  "description": "Film api",
  "private": true,
  "author": "Adam",
  "scripts": {
    "serve": "nodemon --config ./nodemon.config.json",
    "server:build": "rimraf dist/server && tsc -p tsconfig.server.json",
    "test": "npx jest --config ./jest.config.unit.js",
    "down": "docker-compose down -v"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@types/lodash": "^4.14.162",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jest": "^26.6.1",
    "lodash": "^4.17.20",
    "pg-promise": "^10.7.0",
    "ts-jest": "^26.4.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "supertest": "^5.0.0",
    "ts-node": "^8.8.2",
    "typescript": "~3.8.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
